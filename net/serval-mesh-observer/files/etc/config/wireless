config wifi-device  radio0
	option type     mac80211
	option channel  6
	option hwmode	11ng
	option path	'platform/ar933x_wmac'
	option htmode	HT20
	#TO DISABLE WIFI SET TO 1
	option disabled 0

config wifi-iface
	option device   radio0
	option ifname	mon0
	option mode 	monitor
	option htmode	HT20

