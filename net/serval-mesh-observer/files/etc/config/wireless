Config wifi-device  radio0
	option type     mac80211
	option channel  11
	option hwmode	11g
	option path	'platform/ar933x_wmac'
	option htmode	HT20
	#TO DISABLE WIFI SET TO 1
	option disabled 0

config wifi-iface
	option device   radio0
	option network  lan
	option mode     ap
	option ssid     OpenWrt
	option encryption none

